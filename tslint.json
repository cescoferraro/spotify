{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint:react"
  ],
  "jsRules": {},
  "rules": {
    "no-inferrable-types": [
      true,
      "ignore-params",
      "ignore-properties"
    ],
    "no-unused-variable": "off",
    "react/jsx-uses-react": 1
  },
  "rulesDirectory": []
}
