{
  "name": "Spotify",
  "version": "1.0.0",
  "description": "Cutting edge PWA React.js boilerplate.",
  "main": "hot.js",
  "scripts": {
    "refresh": "docker-compose down && docker-compose up -d",
    "clear": "rm -rf dll/ dist/ api/dist",
    "analyzer": "webpack --config webpack.client.js --env.production --env.analyzer",
    "test": "jest --coverage",
    "lint": "tslint --type-check --project tsconfig.json",
    "dll": "webpack --config webpack.dll.js ",
    "web/run": "cross-env NODE_ENV=development node hot.js",
    "web/build": "webpack --config webpack.config.js --env.production",
    "web/deploy": "rm -rf dist/ && yarn web/build && docker-compose build web && yarn refresh",
    "api/run": "VERSION=$npm_package_version reflex -r '\\.go' -s -- ./internal/scripts/runGO.sh",
    "api/build": "VERSION=$npm_package_version ./internal/scripts/buildGO.sh",
    "api/deploy": "rm -rf api/dist/ && yarn api/build && docker-compose build api && yarn refresh",
    "deploy": "yarn web/deploy && yarn api/deploy",
    "start": "concurrently 'yarn web/run' 'yarn api/run'",
    "prod": "concurrently 'yarn web/build' 'yarn api/build'",
    "webpack-defaults": "webpack-defaults"
  },
  "author": "Francesco Antonello Ferraro",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^20.0.8",
    "@types/material-ui": "^0.18.0",
    "@types/offline-js": "^0.7.28",
    "@types/react": "^15.0.35",
    "@types/react-redux": "^4.4.45",
    "awesome-typescript-loader": "^3.2.3",
    "babel-plugin-universal-import": "^1.2.7",
    "cachedfs": "^0.3.3",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.5.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint-config-cesco": "^0.0.2",
    "express": "^4.15.4",
    "extract-css-chunks-webpack-plugin": "^2.0.16",
    "extract-text-webpack-plugin": "^3.0.0",
    "faker": "^4.1.0",
    "favicons-webpack-plugin-cesco": "^0.0.6",
    "file-loader": "^0.11.2",
    "global": "^4.3.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.3.1",
    "jest": "^20.0.4",
    "localforage": "^1.5.0",
    "lodash": "^4.17.13",
    "material-ui": "^0.19.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "noop-webpack-plugin": "^1.0.1",
    "offline-js": "^0.7.19",
    "org": "^0.2.0",
    "org-loader": "^1.0.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.1.2",
    "postcss-simple-vars": "^4.1.0",
    "rapscallion": "^2.1.13",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-flag-icon-css": "^1.0.19",
    "react-flags": "wiredmax/react-flags#9/head",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "3.0.0-beta.6",
    "react-list": "^0.8.7",
    "react-md-spinner": "^0.2.5",
    "react-modal": "^2.2.4",
    "react-pull-to-refresh": "^1.1.1",
